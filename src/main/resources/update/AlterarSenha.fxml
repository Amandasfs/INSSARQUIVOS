<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="inss.gca.mogi.mogi.controller.update.AlterarSenhaController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="79.0" layoutY="30.0" prefHeight="46.0" prefWidth="270.0" text="ALTERE SUA SENHA" textFill="#044793">
            <font>
                <Font name="System Bold" size="28.0" />
            </font>
        </Label>
        <PasswordField fx:id="password" layoutX="67.0" layoutY="107.0" prefHeight="28.0" prefWidth="270.0" promptText="Digite sua senha atual" style="-fx-border-color: #044793;" />
        <PasswordField fx:id="newpassword" layoutX="67.0" layoutY="167.0" prefHeight="28.0" prefWidth="270.0" promptText="Digite uma nova senha" style="-fx-border-color: #044793;" />
        <PasswordField fx:id="repeatpassword" layoutX="67.0" layoutY="225.0" prefHeight="28.0" prefWidth="270.0" promptText="Confirme sua nova senha" style="-fx-border-color: #044793;" />
        <Button fx:id="EnviarButton" layoutX="147.0" layoutY="266.0" onAction="#onTrocar" prefHeight="46.0" prefWidth="87.0" style="-fx-background-color: #044793; -fx-alignment: center; -fx-cursor: hand;" text="ALTERAR" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
        <Label layoutX="67.0" layoutY="87.0" text="Senha atual" textFill="#044793">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <Label layoutX="67.0" layoutY="147.0" text="Nova senha" textFill="#044793">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label layoutX="67.0" layoutY="205.0" text="Confirme a senha" textFill="#044793">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
