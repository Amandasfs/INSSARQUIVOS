<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="156.0" prefWidth="368.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inss.gca.mogi.mogi.controller.select.SelectAltServidorController">
    <children>
        <Label layoutX="57.0" layoutY="14.0" prefHeight="37.0" prefWidth="255.0" text="ALTERAR SERVIDOR" textFill="#044793">
            <font>
                <Font name="System Bold" size="26.0" />
            </font>
        </Label>
        <Label layoutX="60.0" layoutY="51.0" text="Selecione o servidor que será alterado" textFill="#044793">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="inputMatricula" layoutX="80.0" layoutY="76.0" prefHeight="37.0" prefWidth="95.0" promptText="MATRÍCULA" style="-fx-border-color: #044793; -fx-text-fill: #044793;" />
        <Button fx:id="alterarButton" layoutX="186.0" layoutY="76.0" prefHeight="37.0" prefWidth="95.0"
                style="-fx-background-color: #044793; -fx-alignment: center; -fx-cursor: hand;"
                text="ALTERAR" textFill="WHITE" onAction="#confirmar">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
